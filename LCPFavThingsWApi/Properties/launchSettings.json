{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": true,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:58075",
      "sslPort": 44387
    }
  },
  "profiles": {
    "LCPFavThingsWApi": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "https://localhost:5001/swagger",
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SGBDServiceMode": "sqlite"
      }
    },
    "LCPFavThingsWApi_Android": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "applicationUrl": "https://192.168.1.67:5001;http://192.168.1.67:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SGBDServiceMode": "sqlite"
      }
    },
    "LCPFavThingsWApi_AndroidEmu": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "applicationUrl": "https://10.0.2.2:5001;http://10.0.2.2:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SGBDServiceMode": "sqlite"
      }
    },
    "LCPFTWApi Watch": {
      "commandName": "Executable",
      "executablePath": "dotnet.exe",
      "workingDirectory": "$(ProjectDir)",
      "commandLineArgs": "watch run",
      "launchUrl": "https://localhost:5001/swagger",
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SGBDServiceMode": "sqlite"
      }
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "https://localhost:44387/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SGBDServiceMode": "sqlite"
      }
    }
  }
}
